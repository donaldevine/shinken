<h1>Listing product_backlogs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Iteration</th>
    <th>State</th>
    <th>Description</th>
    <th>Reason</th>
    <th>Proposeddate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_backlogs.each do |product_backlog| %>
  <tr>
    <td><%= product_backlog.title %></td>
    <td><%= product_backlog.iteration %></td>
    <td><%= product_backlog.state %></td>
    <td><%= product_backlog.description %></td>
    <td><%= product_backlog.reason %></td>
    <td><%= product_backlog.proposeddate %></td>
    <td><%= link_to 'Show', product_backlog %></td>
    <td><%= link_to 'Edit', edit_product_backlog_path(product_backlog) %></td>
    <td><%= link_to 'Destroy', product_backlog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

