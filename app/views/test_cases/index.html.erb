<h1>Listing test_cases</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Iteration</th>
    <th>State</th>
    <th>Description</th>
    <th>Reason</th>
    <th>Area</th>
    <th>Priority</th>
    <th>State</th>
    <th>Automationstatus</th>
    <th>Activitytype</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @test_cases.each do |test_case| %>
  <tr>
    <td><%= test_case.title %></td>
    <td><%= test_case.iteration %></td>
    <td><%= test_case.state %></td>
    <td><%= test_case.description %></td>
    <td><%= test_case.reason %></td>
    <td><%= test_case.area %></td>
    <td><%= test_case.priority %></td>
    <td><%= test_case.state %></td>
    <td><%= test_case.automationstatus %></td>
    <td><%= test_case.activitytype %></td>
    <td><%= link_to 'Show', test_case %></td>
    <td><%= link_to 'Edit', edit_test_case_path(test_case) %></td>
    <td><%= link_to 'Destroy', test_case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Test case', new_test_case_path %>
